msgid "hide"
msgstr "ascunde"

#: image-regenerate-select-crop.php:1113
msgid "force original"
msgstr "forțează ca original"

#: image-regenerate-select-crop.php:1117
msgid "Crop Position"
msgstr "Poziție tăiere"

